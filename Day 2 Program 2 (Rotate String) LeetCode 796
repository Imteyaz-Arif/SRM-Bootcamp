class Solution {
    public boolean rotateString(String s, String goal) {
        int n=s.length();
        boolean flag=false;
        if(s.equals(goal)){
            flag=true;
            return flag;
        }
        else{
            for(int i=0;i<n;i++){
                String str=s.substring(i)+s.substring(0,i);
                if(str.equals(goal)){
                    flag=true;
                    return flag;
                }
            }
        }
        return flag;
    }
}
